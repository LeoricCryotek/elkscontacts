<odoo>
  <data>
    <record id="view_partner_form_elks_member_num" model="ir.ui.view">
      <field name="name">res.partner.form.elks.member.num</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="arch" type="xml">
        <!-- Insert right after the VAT field -->
        <xpath expr="//field[@name='vat']" position="after">
          <field name="x_detail_member_num" string="Member Number"/>
          <field name="x_is_dues_paid" string="Dues Paid"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
